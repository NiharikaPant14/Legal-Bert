        "import tensorflow as tf\n",
        "from datasets import load_dataset"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "4UpycAxFfUIU"
      },
      "outputs": [],
      "source": [
        "# Load the dataset from Hugging Face\n",
        "dataset = load_dataset(\"rcds/swiss_judgment_prediction\",\"all\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "raiW_7E2fo9S"
      },
      "outputs": [],
      "source": [
        "# Convert the Hugging Face dataset into TensorFlow datasets\n",
        "def create_tf_dataset(split):\n",
        "    # Extract the features you want to use (text and label)\n",
        "    def map_function(example):\n",
        "        return {\n",
        "            'text': example['text'],\n",
        "            'label': example['label']\n",
        "        }\n",
        "\n",
        "    # Map over the split to convert it to a usable format\n",
        "    tf_dataset = dataset[split].map(map_function)\n",
        "\n",
        "    # Convert to tf.data.Dataset\n",
        "    tf_dataset = tf_dataset.to_tf_dataset(\n",
        "        columns=['text'],\n",
        "        label_cols=['label'],\n",
        "        batch_size=32,  # You can adjust the batch size here\n",
        "        shuffle=True if split == 'train' else False\n",
        "    )\n",
        "\n",
        "    return tf_dataset"
      ]
    },
