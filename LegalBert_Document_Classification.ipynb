      "source": [
        "def create_tf_dataset(split, batch_size=32, shuffle=True):\n",
        "    \"\"\"\n",
        "    Create a TensorFlow dataset for a given split ('train', 'validation', etc.).\n",
        "    This function tokenizes the text and prepares inputs and labels for the model.\n",
        "    \"\"\"\n",
        "\n",
        "    # Tokenization function\n",
        "    def tokenize_function(example):\n",
        "        tokenized = tokenizer(\n",
        "            example['text'],  # Access the text field\n",
        "            padding='max_length',  # Pad to max sequence length\n",
        "            truncation=True,  # Truncate sequences longer than max length\n",
        "            max_length=128,  # Define max sequence length\n",
        "            return_tensors=\"tf\"  # Return TensorFlow tensors\n",
        "        )\n",
        "        return {\n",
        "            'input_ids': tokenized['input_ids'][0],  # Extract input_ids\n",
        "            'attention_mask': tokenized['attention_mask'][0],  # Extract attention mask\n",
        "            'label': tf.convert_to_tensor(example['label'], dtype=tf.int32)  # Convert label to int\n",
        "        }\n",
        "\n",
        "    # Apply tokenization to the dataset\n",
        "    tokenized_dataset = dataset[split].map(tokenize_function)\n",
        "\n",
        "    # Define a function to prepare features and labels for the model\n",
        "    def format_for_model(example):\n",
        "        return (\n",
        "            {\n",
        "                'input_ids': example['input_ids'],\n",
        "                'attention_mask': example['attention_mask'],\n",
        "            },\n",
        "            example['label'],\n",
        "        )\n",
        "\n",
        "    # Create a TensorFlow dataset\n",
        "    tf_dataset = tf.data.Dataset.from_generator(\n",
        "        lambda: (format_for_model(ex) for ex in tokenized_dataset),\n",
        "        output_signature=(\n",
        "            {\n",
        "                \"input_ids\": tf.TensorSpec(shape=(128,), dtype=tf.int32),\n",
        "                \"attention_mask\": tf.TensorSpec(shape=(128,), dtype=tf.int32),\n",
        "            },\n",
        "            tf.TensorSpec(shape=(), dtype=tf.int32),\n",
        "        ),\n",
        "    )\n",
        "\n",
        "    # Shuffle and batch the dataset\n",
        "    if shuffle:\n",
        "        tf_dataset = tf_dataset.shuffle(buffer_size=10000)\n",
        "    tf_dataset = tf_dataset.batch(batch_size).prefetch(tf.data.AUTOTUNE)\n",
        "\n",
        "    return tf_dataset"
      ]
